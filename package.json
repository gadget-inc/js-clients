  "license": "MIT",
  "repository": "github:gadget-inc/js-clients",
  "homepage": "https://github.com/gadget-inc/js-clients/tree/main/packages/react",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./auto/polaris": {
      "import": "./dist/esm/auto/polaris/index.js",
      "require": "./dist/cjs/auto/polaris/index.js",
      "default": "./dist/esm/auto/polaris/index.js"
    },
    "./auto/mui": {
      "import": "./dist/esm/auto/mui/index.js",
      "require": "./dist/cjs/auto/mui/index.js",
      "default": "./dist/esm/auto/mui/index.js"
    }
  },
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "rm -rf dist && mkdir -p dist/cjs dist/esm auto/polaris auto/mui && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json && echo '{\"main\": \"../../dist/cjs/auto/polaris/index.js\"}' > auto/polaris/package.json && echo '{\"main\": \"../../dist/cjs/auto/mui/index.js\"}' > auto/mui/package.json && echo '{\"type\": \"module\"}' > dist/esm/package.json && tsc -b tsconfig.cjs.json tsconfig.esm.json ",
    "watch": "tsc -b tsconfig.esm.json --watch --preserveWatchOutput",
    "gql-gen": "graphql-codegen",
    "prepublishOnly": "pnpm build",
    "prerelease": "gitpkg publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
